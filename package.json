{
  "name": "binbillserver",
  "version": "1.0.0",
  "description": "Bin Bill Server",
  "scripts": {
    "setup": "[ -d uploads ] || mkdir uploads",
    "build": "rm -rf dist/ && ./node_modules/.bin/babel src -d dist/",
    "start": "node dist/index.js --max_old_space_size=2048",
    "dev": "export $(cat .env | grep -v ^# | xargs) && babel-node  ./src/index.js"
  },
  "dependencies": {
    "@google/maps": "^0.4.6",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "cls-bluebird": "^2.1.0",
    "continuation-local-storage": "^3.2.1",
    "dateformat": "^3.0.3",
    "file-type": "^8.1.0",
    "google-libphonenumber": "^3.2.1",
    "guid": "0.0.12",
    "hapi": "^17.8.1",
    "hapi-auth-jwt2": "^8.1.0",
    "hapi-cors": "^1.0.1",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "joi": "^13.7.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.18",
    "moment": "^2.22.2",
    "moment-weekday-calc": "^1.1.3",
