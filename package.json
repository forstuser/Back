    "socket.io": "^2.2.0",
    "url-join": "^4.0.0",
    "uuid": "^3.2.1",
    "validator": "^10.2.0",
    "vision": "^5.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
